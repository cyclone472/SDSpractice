package P9663;

import java.io.FileInputStream;
import java.io.FileNotFoundException;
import java.util.Scanner;

public class Main {
	static int N;
	static boolean[][] board;
	
	public static void main(String[] args) throws FileNotFoundException {
		// TODO Auto-generated method stub
		System.setIn(new FileInputStream("src\\P9663\\input.txt"));
		Scanner sc = new Scanner(System.in);
		
		N = Integer.parseInt(sc.next());
		board = new boolean[N][N];
		
		for(int i = 0; i < N; i++) {
			for(int j = 0; j < N; j++) {
				// board에 queen을 놓을 수 있으면 true
				board[j][i] = true;
			}
		}
		
		System.out.println(solve(0));
	}
	
	static int solve(int row) {
		if(row == N) {
			return 1;
		}
		
		int ret = 0;
		boolean[][] initBoard = new boolean[N][N];
		for (int y = 0; y < N; y++) {
			for (int x = 0; x < N; x++) {
				initBoard[y][x] = board[y][x];
			}
		}
		
		for(int i = 0; i < N; i++) {
			if(board[row][i] == true) {
				// board를 paint
				for (int y = row + 1, j = 1; y < N; y++, j++) {
					board[y][i] = false;
					if (i - j >= 0)
						board[y][i-j] = false;
					if (i + j < N)
						board[y][i+j] = false;
				}
				
				ret += solve(row + 1);
				for (int y = 0; y < N; y++) {
					for (int x = 0; x < N; x++) {
						board[y][x] = initBoard[y][x];
					}
				}
			}
		}		
		return ret;
	}
	
	static boolean[][] paint(boolean[][] board, int row, int col) {
		for (int y = row + 1, i = 1; y < N; y++, i++) {
			board[y][col] = false;
			if (col - i >= 0)
				board[y][col-i] = false;
			if (col + i < N)
				board[y][col+i] = false;
		}
		
		return board;
	}
}
